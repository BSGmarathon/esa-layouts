{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "playing": {
      "type": "boolean",
      "default": false
    },
    "paused": {
      "type": "boolean",
      "default": false
    },
    "sum": {
      "type": "string"
    },
    "position": {
      "type": "number"
    },
    "metadata": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "title": {
          "type": "string"
        },
        "artist": {
          "type": "string"
        }
      }
    },
    "history": {
      "type": "object",
      "additionalProperties": {
        "type": "number"
      }
    },
    "recent": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": []
    }
  },
  "required": [
    "playing",
    "paused",
    "metadata",
    "history",
    "recent"
  ]
}