{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "rotation": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "sum": {
            "type": "string"
          },
          "id": {
            "type": "string"
          },
          "seconds": {
            "type": "number"
          }
        },
        "required": [
          "sum",
          "id",
          "seconds"
        ]
      },
      "default": []
    },
    "current": {
      "type": "string"
    }
  },
  "required": [
    "rotation"
  ]
}