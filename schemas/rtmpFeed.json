{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "server": {
      "type": "string",
      "enum": [
        "eu",
        "na"
      ],
      "default": "eu"
    },
    "editAllowed": {
      "type": "boolean",
      "default": true
    },
    "enabled": {
      "type": "boolean",
      "default": true
    },
    "streamKey": {
      "type": "string",
      "default": ""
    },
    "feedIndex": {
      "type": "number",
      "default": 0
    }
  },
  "required": [
    "server",
    "enabled",
    "feedIndex",
    "streamKey",
    "editAllowed"
  ]
}
